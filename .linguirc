{
  "catalogs": [
    {
      "path": "<rootDir>/app/locales/{locale}/messages",
      "include": [
        "<rootDir>/app"
      ],
      "exclude": [
        "**/node_modules/**",
        "**/.next/**"
      ]
    }
  ],
  "locales": [
    "de",
    "fr",
    "it",
    "en"
  ],
  "format": "po",
  "compileNamespace": "es",
  "sourceLocale": "en",
  "fallbackLocales": {
    "default": "en"
  }
}
