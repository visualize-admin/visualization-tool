{
  "baseUrl": "http://localhost:3333",
  "ignoreTestFiles": ["**/__snapshots__/*", "**/__image_snapshots__/*"],
  "env": {
    "cypress-plugin-snapshots": {
      "autoCleanUp": true,
      "imageConfig": {
        "createDiffImage": true,
        "resizeDevicePixelRatio": true,
        "threshold": 0.05,
        "thresholdType": "percent"
      }
    }
  }
}
