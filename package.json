{
  "name": "visualization-tool",
  "version": "1.0.0",
  "repository": "git@gitlab.ldbar.ch:interactivethings/visualization-tool.git",
  "author": "Jeremy Stucki <jeremy@interactivethings.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "DATABASE_URL=postgres://postgres@localhost:5432/visualization_tool next ./app",
    "db:migrate:dev": "DATABASE_URL=postgres://postgres@localhost:5432/visualization_tool scripts/db-migrate.js",
    "db:migrate": "scripts/db-migrate.js",
    "build": "NODE_OPTIONS=--max_old_space_size=2048 lingui compile && next build ./app",
    "start": "yarn db:migrate && next start ./app -p $PORT",
    "lint": "tsc --noEmit -p ./app",
    "locales:extract": "lingui extract",
    "locales:compile": "lingui compile",
    "setup:dev": "yarn && yarn locales:compile && yarn db:migrate:dev"
  },
  "dependencies": {
    "@babel/standalone": "^7.6.2",
    "@lingui/react": "^2.8.3",
    "@mdx-js/react": "^1.5.0",
    "@next/bundle-analyzer": "^9.0.7",
    "@next/mdx": "^9.0.6",
    "@rebass/forms": "^4.0.5",
    "@rebass/preset": "^4.0.5",
    "@zazuko/query-rdf-data-cube": "^0.5.0",
    "catalog": "^4.0.1-canary.2",
    "d3-array": "^2.3.3",
    "d3-format": "^1.4.2",
    "downshift": "^3.4.1",
    "emotion-theming": "^10.0.17",
    "fp-ts": "^2.0.5",
    "immer": "^5.0.0",
    "io-ts": "^2.0.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "nanoid": "^2.1.0",
    "next": "^9.0.5",
    "pg": "^7.12.1",
    "pg-connection-string": "^2.1.0",
    "postgres-migrations": "^3.0.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rebass": "^4.0.5",
    "resize-observer-polyfill": "^1.5.1",
    "swr": "^0.1.9",
    "use-immer": "^0.3.3",
    "vega": "^5.6.0"
  },
  "devDependencies": {
    "@lingui/cli": "^2.8.3",
    "@lingui/macro": "^2.8.3",
    "@mdx-js/loader": "^1.5.0",
    "@types/d3-array": "^2.0.0",
    "@types/d3-format": "^1.3.1",
    "@types/lingui__core": "^2.7.0",
    "@types/lingui__macro": "^2.7.3",
    "@types/lingui__react": "^2.8.0",
    "@types/lodash": "^4.14.138",
    "@types/nanoid": "^2.0.0",
    "@types/node": "^12.7.2",
    "@types/pg": "^7.11.1",
    "@types/react": "^16.9.2",
    "@types/rebass": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-plugin-macros": "^2.6.1",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.3.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.2"
