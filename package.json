{
  "name": "visualization-tool",
  "version": "0.2.2",
  "repository": "git@gitlab.ldbar.ch:interactivethings/visualization-tool.git",
  "author": "Jeremy Stucki <jeremy@interactivethings.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "DATABASE_URL=postgres://postgres@localhost:5432/visualization_tool next ./app",
    "dev:rollup": "rollup -c rollup.config.js --watch",
    "db:migrate:dev": "DATABASE_URL=postgres://postgres@localhost:5432/visualization_tool scripts/db-migrate.js",
    "db:migrate": "scripts/db-migrate.js",
    "build": "NODE_OPTIONS=--max_old_space_size=2048 lingui compile && rollup -c rollup.config.js && next build ./app",
    "start": "yarn db:migrate && next start ./app -p $PORT",
    "lint": "tsc --noEmit -p ./app -p ./embed",
    "locales:extract": "lingui extract",
    "locales:compile": "lingui compile",
    "setup:dev": "yarn && yarn locales:compile && yarn db:migrate:dev",
    "preversion": "yarn lint",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@babel/standalone": "^7.7.6",
    "@lingui/react": "^2.8.3",
    "@mdx-js/react": "^1.5.2",
    "@next/bundle-analyzer": "^9.1.6",
    "@next/mdx": "^9.1.6",
    "@rebass/forms": "^4.0.5",
    "@rebass/preset": "^4.0.5",
    "@zazuko/query-rdf-data-cube": "^0.7.0",
    "catalog": "^4.0.1-canary.2",
    "clipboard-polyfill": "^2.8.6",
    "d3-array": "^2.3.3",
    "d3-dsv": "^1.2.0",
    "d3-format": "^1.4.2",
    "downshift": "^3.4.8",
    "emotion-theming": "^10.0.17",
    "file-saver": "^2.0.2",
    "fp-ts": "^2.3.1",
    "iframe-resizer": "^4.2.8",
    "immer": "^5.0.2",
    "io-ts": "^2.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "nanoid": "^2.1.8",
    "next": "^9.1.6",
    "pg": "^7.15.0",
    "pg-connection-string": "^2.1.0",
    "postgres-migrations": "^3.0.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rebass": "^4.0.5",
    "resize-observer-polyfill": "^1.5.1",
    "swr": "^0.1.14",
    "use-immer": "^0.3.3",
    "vega": "^5.6.0"
  },
  "devDependencies": {
    "@lingui/cli": "^2.8.3",
    "@lingui/macro": "^2.8.3",
    "@mdx-js/loader": "^1.5.2",
