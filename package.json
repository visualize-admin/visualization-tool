{
  "name": "@visualize-admin/visualization-tool",
  "version": "0.5.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/visualize-admin/visualization-tool.git"
  },
  "author": "Federal Office for the Environment FOEN <visualize@bafu.admin.ch>",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "dev": "preconstruct dev && next ./app",
    "dev:mitmproxy": "preconstruct dev && HTTP_PROXY=http://localhost:8080 NODE_EXTRA_CA_CERTS=~/.mitmproxy/mitmproxy-ca-cert.pem GLOBAL_AGENT_ENVIRONMENT_VARIABLE_NAMESPACE='' NO_PROXY='localhost,127.0.0.1' next ./app",
    "dev:rollup": "rollup -c rollup.config.js --watch",
    "db:migrate:dev": "DATABASE_URL=postgres://postgres@localhost:5432/visualization_tool scripts/db-migrate.js",
    "db:migrate": "scripts/db-migrate.js",
    "build": "NODE_OPTIONS=--max_old_space_size=2048 yarn graphql:codegen && lingui compile && rollup -c rollup.config.js && next build ./app",
    "build:npm": "yarn graphql:codegen && lingui compile && BABEL_ENV=NPM_PACKAGE preconstruct build",
    "start": "yarn db:migrate && GLOBAL_AGENT_ENVIRONMENT_VARIABLE_NAMESPACE='' NO_PROXY='localhost,127.0.0.1' next start ./app -p $PORT",
    "lint": "tsc --noEmit -p ./app -p ./embed",
    "locales:extract": "lingui extract --verbose",
    "locales:compile": "lingui compile --verbose",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "graphql:codegen:dev": "graphql-codegen --config codegen.yml --watch",
    "setup:dev": "yarn && yarn locales:compile && yarn db:migrate:dev",
    "postversion": "git push --follow-tags",
    "release:npm": "yarn build:npm && yarn publish app",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "next": "^9.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/typescript": "^1.13.5",
    "@graphql-codegen/typescript-operations": "^1.13.5",
    "@graphql-codegen/typescript-resolvers": "^1.13.5",
    "@graphql-codegen/typescript-urql": "^1.13.5",
    "@lingui/cli": "^2.8.3",
    "@lingui/macro": "^2.8.3",
    "@mdx-js/loader": "^1.6.1",
    "@preconstruct/cli": "^1.1.7",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/pluginutils": "^3.0.10",
    "@types/cors": "^2.8.6",
    "@types/d3-array": "^2.0.0",
    "@types/d3-axis": "^1.0.12",
    "@types/d3-color": "^1.2.2",
    "@types/d3-delaunay": "^4.1.0",
    "@types/d3-dsv": "^1.0.36",
    "@types/d3-format": "^1.3.1",
    "@types/d3-scale": "^2.1.1",
    "@types/d3-scale-chromatic": "^1.3.1",
    "@types/d3-selection": "^1.4.1",
    "@types/d3-shape": "^1.3.2",
    "@types/d3-time": "^1.0.10",
    "@types/d3-time-format": "^2.1.1",
    "@types/file-saver": "^2.0.1",
    "@types/fs-extra": "^8.1.0",
    "@types/fuzzaldrin-plus": "^0.6.0",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/lingui__core": "^2.7.0",
    "@types/lingui__macro": "^2.7.3",
    "@types/lingui__react": "^2.8.0",
    "@types/lodash": "^4.14.138",
    "@types/micro-cors": "^0.1.0",
    "@types/node": "^12.12.31",
    "@types/pg": "^7.11.1",
    "@types/puppeteer": "^2.0.1",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.4",
    "@types/theme-ui": "^0.3.1",
    "@types/theme-ui__components": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "babel-eslint": "10.x",
    "babel-jest": "^26.0.1",
    "babel-plugin-macros": "^2.8.0",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "fs-extra": "^9.0.0",
    "jest": "^26.0.1",
    "jest-image-snapshot": "^3.0.1",
    "jest-puppeteer": "^4.4.0",
    "prettier": "^2.0.5",
    "puppeteer": "^3.0.3",
    "rollup": "^2.8.0",
    "rollup-plugin-analyzer": "^3.2.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-terser": "^5.1.3",
    "typescript": "3.7.x"
  },
  "workspaces": [
    "app"
  ],
  "preconstruct": {
    "packages": [
      "app"
    ]
  },
  "cacheDirectories": [
    "node_modules",
    "app/.next/cache"
  ]
}
